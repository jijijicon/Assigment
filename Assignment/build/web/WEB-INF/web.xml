<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertStudentControll</servlet-name>
        <servlet-class>Controller.InsertStudentControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchStudentControll</servlet-name>
        <servlet-class>Controller.SearchStudentControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>Controller.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentDetailControll</servlet-name>
        <servlet-class>Controller.StudentDetailControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStudentControll</servlet-name>
        <servlet-class>Controller.DeleteStudentControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStudentControll</servlet-name>
        <servlet-class>Controller.UpdateStudentControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListMarkControll</servlet-name>
        <servlet-class>Controller.Mark.ListMarkControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailMarkControll</servlet-name>
        <servlet-class>Controller.Mark.DetailMarkControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateMarkControll</servlet-name>
        <servlet-class>Controller.Mark.UpdateMarkControll</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertStudentControll</servlet-name>
        <url-pattern>/student/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchStudentControll</servlet-name>
        <url-pattern>/student/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentDetailControll</servlet-name>
        <url-pattern>/student/infor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStudentControll</servlet-name>
        <url-pattern>/student/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStudentControll</servlet-name>
        <url-pattern>/student/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListMarkControll</servlet-name>
        <url-pattern>/mark/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailMarkControll</servlet-name>
        <url-pattern>/mark/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateMarkControll</servlet-name>
        <url-pattern>/mark/update</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
